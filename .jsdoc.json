{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "source": {
        "include": [
            "src",
            "package.json",
            "README.md"
        ],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "sourceType": "module",
    "recurseDepth": 10,
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "useLongnameInNav": true,
        "showInheritedInNav": true
    },
    "opts": {
        "destination": "./docs/",
        "encoding": "utf8",
        "readme": "./README.md",
        "recurse": true,
        "verbose": true,
        "template": "node_modules/clean-jsdoc-theme",
        "theme_opts": {
            "search": true,
            "footer": "Powered by Ninju",
            "default_theme": "dark",
            "base_url": "https://ninjujs.netlify.app/",
            "favicon": "https://github.com/ninju69/ninju.js/blob/master/assets/Img/icon.png",
            "homepageTitle": "NinjuJS",
            "title": "ninjujs",
            "menu": [
                {
                    "title": "Github",
                    "id": "github",
                    "link": "https://github.com/ninju69/ninju.js"
                },
                {
                    "title": "NPM",
                    "id": "npm",
                    "link": "https://www.npmjs.com/package/ninju.js"
                },
                {
                    "title": "Discord",
                    "id": "discord",
                    "link": "https://dsc.gg/teamcrypts"
                }
            ],
            "meta": [
                {
                    "name": "author",
                    "content": "NinjuJS"
                },
                {
                    "name": "description",
                    "content": "A simple, lightweight, and easy to use package for discord.js!"
                }
            ],
            "codepen": {
                "enable_for": [
                    "examples"
                ],
                "options": {
                    "js_external": "https://code.jquery.com/jquery-3.6.0.min.js",
                    "js_pre_processor": "babel"
                }
            },
            "sections": [
                "Classes",
                "Modules",
                "Global"
            ]
        }
    },
        "markdown": {
        "hardwrap": false,
        "idInHeadings": true
    }
}
